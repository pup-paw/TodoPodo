            android:layout_marginRight="-2dp"
            android:layout_toLeftOf="@id/btn4"
            android:background="@drawable/circlebtn"
            android:text="5" />

        <Button
            android:id="@+id/btn6"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_toLeftOf="@id/btn5"
